"Resource/layout/overlaydialogsettings.layout" {

	layout {
		region { name=header height=50 width=max }
			place { control=frame_close height=40 width=40 align=right  }
			place { control=frame_captiongrip margin=2 width=max height=50 }

		region { name=body height=max width=max y=50 margin-bottom=50 }
		region { name=box height=max width=max region=body margin-left=20 margin-right=26 }
			place { control=sheet region=box height=max width=max }

		region { name=buttons align=bottom height=50 }
			place { control=OKButton,CancelButton region=buttons width=100 align=right spacing=10 y=10 margin-right=16 }

		place { control=frame_minimize height=0 width=0 margin-left=-9999 }
	}
}

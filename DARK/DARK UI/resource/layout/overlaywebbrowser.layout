"Steam/cached/overlaywebbrowser.layout" {




	layout {
		//region { name=header height=30 width=max }
			place [!$OSX] { control=frame_close height=22 width=22 y=13 align=right margin-right=20 }
			place [$OSX] { control=frame_close height=22 width=22 y=13 align=left margin-left=20 }
			place { control=frame_captiongrip margin=2 width=max height=50 }

		region { name=nav height=30 width=max x=8 y=8 }
		region { name=body height=max width=max y=46 }

		place {
			control=BackButton,ForwardButton,HomeButton
			region=nav height=30 width=30 spacing=4
		}

		place {
			control=ReloadButton,StopButton
			region=nav height=30 spacing=10 start=ForwardButton
		}

		place {
			control=URLBar
			region=nav y=4 x=126 height=22 width=max margin-right=8
		}

		place {
			control=PageLoadThrobber
			region=nav align=right height=0 width=0
		}

		place { control=HTMLField region=body width=max height=max }

		place { control=HomeButton,StatusText,URLStatusImage height=0 width=0 margin-left=-9999 }
	}
}
